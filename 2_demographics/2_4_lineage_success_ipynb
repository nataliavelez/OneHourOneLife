    "\n",
    "start_t = seed_t[0]\n",
    "\n",
    "print('Found %i seed files' % len(seed_t))\n",
    "print('Starting lifelogs at: %s' % datetime.datetime.fromtimestamp(start_t).strftime('%c'))\n",
    "print(*seed_t, sep='\\n')"
